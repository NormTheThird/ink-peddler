@using System.Web.Optimization
<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="Flavor Mob">
    <title>Ink Peddler Admin</title>

    @*@Styles.Render("~/Content/core_styles")*@
    @*<link rel="shortcut icon" href="assets/images/favicon.ico">*@

    <link href="~/Content/plugins/jquery-circliful/css/jquery.circliful.css" rel="stylesheet" type="text/css" />

    <!-- Dropzone css -->
    <link href="~/Content/plugins/dropzone/dropzone.css" rel="stylesheet" />

    <!-- jvectormap -->
    <link href="~/Content/plugins/jvectormap/jquery-jvectormap-2.0.2.css" rel="stylesheet" />

    <!-- App css -->
    <link href="~/Content/css/bootstrap.min.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/icons.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/css/style_dark.css" rel="stylesheet" type="text/css" />

    <script src="~/Content/js/modernizr.min.js"></script>
</head>
<body class="fixed-left">

    @if (User?.Identity == null || !User.Identity.IsAuthenticated)
    {
        @RenderBody()
    }
    else
    {
        <div id="wrapper">

            @Html.Partial("_LayoutHeader")

            <div class="content-page">

                <div class="content">
                    <div class="container-fluid">
                        @RenderBody()
                    </div>
                </div>

                @Html.Partial("_LayoutFooter")

            </div>
        </div>

        <div class="left side-menu">
            <div class="sidebar-inner slimscrollleft">
                <div id="sidebar-menu">
                    @Html.Partial("_LayoutNavigationAdmin")
                    @Html.Partial("_LayoutNavigationUser")
                </div>
            </div>
        </div>
    }

    <!-- jQuery  -->
    <script>var resizefunc = [];</script>
    <script src="~/Content/js/jquery.min.js"></script>
    <script src="~/Content/js/popper.min.js"></script>
    <script src="~/Content/js/bootstrap.min.js"></script>
    <script src="~/Content/js/detect.js"></script>
    <script src="~/Content/js/fastclick.js"></script>
    <script src="~/Content/js/jquery.slimscroll.js"></script>
    <script src="~/Content/js/jquery.blockUI.js"></script>
    <script src="~/Content/js/waves.js"></script>
    <script src="~/Content/js/wow.min.js"></script>
    <script src="~/Content/js/jquery.scrollTo.min.js"></script>
    <script src="~/Content/plugins/notify/notify.min.js"></script>
    <script src="~/Content/plugins/knockout/knockout.3.4.2.min.js"></script>

    <!-- Counter Up  -->
    <script src="~/Content/plugins/waypoints/lib/jquery.waypoints.min.js"></script>
    <script src="~/Content/plugins/counterup/jquery.counterup.min.js"></script>

    <!-- circliful Chart -->
    <script src="~/Content/plugins/jquery-circliful/js/jquery.circliful.min.js"></script>
    <script src="~/Content/plugins/jquery-sparkline/jquery.sparkline.min.js"></script>

    <!-- skycons -->
    <script src="~/Content/plugins/skyicons/skycons.min.js" type="text/javascript"></script>

    <!-- Page js  -->
    <script src="~/Content/pages/jquery.dashboard.js"></script>

    <!-- Dropzone js -->
    <script src="~/Content/plugins/dropzone/dropzone.js"></script>

    <!-- AWS S3-->
    <script src="https://sdk.amazonaws.com/js/aws-sdk-2.337.0.min.js"></script>

    <!-- jvectormap -->
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-2.0.2.min.js"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-world-mill-en.js"></script>
    <script src="~/Content/plugins/jvectormap/gdp-data.js"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-us-aea-en.js"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-uk-mill-en.js"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-au-mill.js"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-us-il-chicago-mill-en.js"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-ca-lcc.js"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-de-mill.js"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-in-mill.js"></script>
    <script src="~/Content/plugins/jvectormap/jquery-jvectormap-asia-mill.js"></script>

    <!-- App js -->
    <script src="~/Content/js/jquery.core.js"></script>
    <script src="~/Content/js/jquery.app.js"></script>

    @Scripts.Render("~/bundles/view_models")
    <script type="text/javascript">
        var MainVM = new MainViewModel();
        ko.applyBindings(MainVM);
    </script>

    @RenderSection("scripts", false)
</body>
</html>